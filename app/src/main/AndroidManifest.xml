<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.ufrn.sigestagios">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.OfferActivity" />
        <activity android:name=".activities.RegistrationFormActivity" />
        <activity android:name=".activities.LoginActivity"></activity>

        <activity android:name=".activities.AssociatedActionShowActivity"></activity>
        <activity android:name=".activities.ExtensionShowActivity"></activity>
        <activity android:name=".activities.InternshipShowActivity"></activity>
        <activity android:name=".activities.ResearchGrantShowActivity"></activity>
        <activity android:name=".activities.SupportServiceShowActivity"></activity>
        <activity android:name=".activities.TeacherAssistantShowActivity"></activity>


    </application>

</manifest>